<h1>Create a new Heroine</h1>

<% if @errors %>
  <% @errors.each do |e| %>
  <%= e %>
  <% end %>
<% end %>

<%= form_for @heroine do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :super_name %>
<%= f.text_field :super_name %>
<%= f.collection_select :power_id, @powers, :id, :name %>
<%= f.submit %>
<% end %>
