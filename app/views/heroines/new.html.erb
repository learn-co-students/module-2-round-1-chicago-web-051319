<% if @errors %>
  <% @errors.each do |error| %>
    <p><%= error %></p>
  <% end %>
<% end %>

<%= form_for(@heroine) do |f| %>
<%= f.label :name %><br>
 <%= f.text_field :name %><br>

<%= f.label :super_name %><br>
<%= f.text_field :super_name %><br>
<%=f.collection_select :power_id, @powers, :id, :name %> <br>
<%= f.submit %>
<%end%>
