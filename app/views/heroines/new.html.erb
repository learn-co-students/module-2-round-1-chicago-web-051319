<h1>New Heroine</h1>
<% if @errors %>
  <% @errors.each do |error|%>
  <h2><%= error %></h2>
  <% end %>
<% end %>
<%= form_for @heroine do |f|%>
  <p><%= f.label :super_name%>
  <%= f.text_field :super_name%></p>
  <p><%= f.label :name%>
  <%= f.text_field :name%></p>
  <%= f.collection_select :power_id, @powers, :id, :name %>
  <%= f.submit %>
<% end %>
